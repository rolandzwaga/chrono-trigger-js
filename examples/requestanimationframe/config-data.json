                        "systemName": "SubtitlesController"
                    }
                },
                {
                    "systemName": "addControllerToElement",
                    "operationData": {
                        "language": "config:language",
                        "subtitleData": "json:testSubtitles"
                    }
                }
            ],
            "endOperations": [
                {
                    "systemName": "selectElement",
                    "operationData": {
                        "selector": "#subtitles"
                    }
                },
                {
                    "systemName": "removeControllerFromElement",
                    "operationData": {
                        "systemName": "SubtitlesController"
                    }
                }
            ]
        }
