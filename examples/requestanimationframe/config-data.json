                    "systemName": "selectElement",
                    "operationData": {
                        "selector": "#langsel"
                    }
                },
                {
                    "systemName": "addOptionList",
                    "operationData": {
                        "optionData": "config:availableLanguages",
                        "labelProperty": "label",
                        "valueProperty": "code",
                        "defaultValue": "nl_NL"
                    }
                },
                {
                    "systemName": "getControllerInstance",
                    "operationData": {
                        "systemName": "EventListenerController"
                    }
                },
                {
                    "systemName": "addControllerToElement",
                    "operationData": {
                        "eventName": "change",
                        "actions": [
                            "BroadcastLanguageChange"
                        ]
                    }
                }
            ],
            "endOperations": [
                {
                    "systemName": "selectElement",
                    "operationData": {
                        "selector": "#langsel"
                    }
                },
                {
                    "systemName": "removeControllerFromElement",
                    "operationData": {
                        "systemName": "EventListenerController"
                    }
                }
            ]
        },
        {
