                {
                    "systemName": "getControllerInstance",
                    "operationData": {
                        "systemName": "ProgressbarController"
                    }
                },
                {
                    "systemName": "addControllerToElement",
                    "operationData": {
                        "playerId": "config:timelineProviderSettings.selector"
