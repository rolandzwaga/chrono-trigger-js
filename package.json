{
  "name": "chrono-trigger-js",
  "version": "1.5.8",
  "description": "An engine that allows arbitrary functionality, defined by a JSON configuration, to be triggered according to a given timeline provider",
  "main": "./lib/library.min.js",
  "types": "./lib/library.d.ts",
  "repository": "https://github.com/rolandzwaga/chrono-trigger.git",
  "author": "Roland Zwaga <rbzwaga@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "mochapack --glob \"*.spec.js\" test --recursive --require test/setup.js --webpack-config test/webpack.config.js --watch",
    "test-ci": "mochapack --glob \"*.spec.js\" test --recursive --webpack-config test/webpack.config.js",
    "build-example-1": "node build/engine-pack-builder examples/requestanimationframe/config-data.json && webpack --config build/webpack.config.js --env.entryPath examples/requestanimationframe/build/boot.js",
    "full-build": "npm run build && npm run dev-build",
    "build": "webpack --mode production --display-modules",
    "dev-build": "webpack --mode development --progress --colors --display-modules",
    "dev": "webpack --mode development --progress --colors --display-modules --watch",
    "stage-build": "git add lib/library.js lib/library.js.map lib/library.min.js lib/library.min.js.map lib/library.d.ts",
    "publish-patch": "npm run full-build && npm run stage-build && git commit -m\"Add new patch build\" && npm version patch",
    "publish-minor": "npm run full-build && npm run stage-build && git commit -m\"Add new minor build\"  && npm version minor",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@babel/plugin-transform-regenerator": "7.8.3",
    "@babel/runtime": "7.8.4",
    "d3": "5.15.0",
    "es6-shim": "0.35.5",
    "jquery": "3.4.1",
    "lottie-web": "5.6.5",
    "mediaelement": "4.2.14",
    "mousetrap": "1.6.5",
    "uuid": "7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-arrow-functions": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.8.3",
    "@babel/plugin-transform-object-assign": "7.8.3",
    "@babel/plugin-transform-parameters": "7.8.4",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@types/node": "13.7.4",
    "acorn": "7.1.0",
    "ajv": "6.12.0",
    "astring": "1.4.3",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.0.6",
    "canvas": "^2.6.1",
    "chai": "4.2.0",
    "child_process": "1.0.2",
    "chokidar": "3.3.1",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.4.2",
    "fast-async": "6.3.8",
    "file-loader": "5.1.0",
    "get-random-values": "1.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "inject-loader": "4.0.1",
    "jasmine": "3.5.0",
    "jsdom": "16.2.0",
    "jsdom-global": "3.0.2",
    "mocha": "7.0.1",
    "mochapack": "1.1.13",
    "node-sass": "4.13.1",
    "nodemon": "2.0.2",
    "parse5": "5.1.1",
    "postcss": "7.0.27",
    "postcss-loader": "3.0.0",
    "pretty": "2.0.0",
    "raf-stub": "3.0.0",
    "randomstring": "^1.1.5",
    "sass-loader": "8.0.2",
    "sinon": "9.0.0",
    "style-loader": "1.1.3",
