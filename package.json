{
  "name": "chrono-trigger-js",
  "version": "1.6.7",
  "description": "An engine that allows arbitrary functionality, defined by a JSON configuration, to be triggered according to a given timeline provider",
  "main": "./dist/library.min.js",
  "types": "./dist/src/index.d.ts",
  "repository": "https://github.com/rolandzwaga/chrono-trigger.git",
  "author": "Roland Zwaga <rbzwaga@gmail.com>",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --pretty --noEmit",
    "test": "karma start",
    "test-ci": "jasmine-ts",
    "build-example-1": "node build/engine-pack-builder examples/requestanimationframe/config-data.json && webpack --config build/webpack.config.js --env.entryPath examples/requestanimationframe/build/boot.js",
    "build-example-2": "node build/engine-pack-builder examples/mediaelement/config-data.json && webpack --config build/webpack.config.js --env.entryPath examples/mediaelement/build/boot.js",
    "full-build": "npm run build && npm run dev-build",
    "build": "webpack --mode production",
    "dev-build": "webpack --mode development --progress --colors",
    "dev": "webpack --mode development --progress --colors --display-modules --watch",
    "stage-build": "git add lib/library.js lib/library.js.map lib/library.min.js lib/library.min.js.map lib/library.d.ts",
    "publish-patch": "yarn full-build && yarn stage-build && git commit -m\"Add new patch build\" && npm version patch",
    "publish-minor": "yarn full-build && yarn stage-build && git commit -m\"Add new minor build\"  && npm version minor",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "d3": "6.5.0",
    "es6-shim": "0.35.6",
    "jquery": "3.5.1",
    "lottie-web": "5.7.6",
    "mediaelement": "4.2.16",
    "mousetrap": "1.6.5",
    "ts-is-present": "1.2.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/jasmine": "3.6.3",
    "@types/jquery": "3.5.5",
    "@types/mousetrap": "1.6.5",
    "@types/node": "14.14.25",
    "@types/sinon": "9.0.10",
    "@types/uuid": "8.3.0",
    "acorn": "8.0.5",
    "ajv": "7.0.4",
    "astring": "1.7.0",
    "autoprefixer": "10.2.4",
    "awesome-typescript-loader": "5.2.1",
    "canvas": "2.6.1",
    "chai": "4.3.0",
    "child_process": "1.0.2",
    "chokidar": "3.5.1",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "fast-async": "6.3.8",
    "file-loader": "6.2.0",
    "fs-extra": "9.1.0",
    "get-random-values": "1.2.2",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "5.0.0",
    "inject-loader": "4.0.1",
    "jasmine-core": "3.6.0",
    "karma": "6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "2.0.0",
    "karma-jasmine": "4.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "5.2.0",
    "node-sass": "5.0.0",
    "nodemon": "2.0.7",
    "parse5": "6.0.1",
    "postcss": "8.2.5",
    "postcss-loader": "5.0.0",
    "pretty": "2.0.0",
    "raf-stub": "3.0.0",
    "randomstring": "1.1.5",
    "sass-loader": "11.0.0",
    "sinon": "9.2.4",
    "style-loader": "2.0.0",
    "svg-inline-loader": "0.8.2",
    "terser-webpack-plugin": "5.1.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "4.1.3",
    "webpack": "5.21.1",
    "webpack-cli": "4.5.0"
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ]
}
