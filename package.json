{
  "name": "chrono-trigger-js",
  "version": "1.6.7",
  "description": "An engine that allows arbitrary functionality, defined by a JSON configuration, to be triggered according to a given timeline provider",
  "main": "./lib/library.min.js",
  "types": "./lib/library.d.ts",
  "repository": "https://github.com/rolandzwaga/chrono-trigger.git",
  "author": "Roland Zwaga <rbzwaga@gmail.com>",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --pretty --noEmit",
    "test": "mochapack --glob \"*.spec.js\" src/test --recursive --require src/test/setup.js --webpack-config src/test/webpack.config.js --watch",
    "test-ci": "mochapack --glob \"*.spec.js\" test --recursive --webpack-config test/webpack.config.js",
    "build-example-1": "node build/engine-pack-builder examples/requestanimationframe/config-data.json && webpack --config build/webpack.config.js --env.entryPath examples/requestanimationframe/build/boot.js",
    "build-example-2": "node build/engine-pack-builder examples/mediaelement/config-data.json && webpack --config build/webpack.config.js --env.entryPath examples/mediaelement/build/boot.js",
    "full-build": "npm run build && npm run dev-build",
    "build": "webpack --mode production --display-modules",
    "dev-build": "webpack --mode development --progress --colors --display-modules",
    "dev": "webpack --mode development --progress --colors --display-modules --watch",
    "stage-build": "git add lib/library.js lib/library.js.map lib/library.min.js lib/library.min.js.map lib/library.d.ts",
    "publish-patch": "yarn full-build && yarn stage-build && git commit -m\"Add new patch build\" && npm version patch",
    "publish-minor": "yarn full-build && yarn stage-build && git commit -m\"Add new minor build\"  && npm version minor",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "d3": "6.2.0",
    "es6-shim": "0.35.6",
    "jquery": "3.5.1",
    "lottie-web": "5.7.4",
    "mediaelement": "4.2.16",
    "mousetrap": "1.6.5",
    "ts-is-present": "^1.1.5",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@types/jquery": "3.5.4",
    "@types/mocha": "^8.0.3",
    "@types/mousetrap": "1.6.4",
    "@types/node": "14.14.6",
    "@types/uuid": "8.3.0",
    "acorn": "8.0.4",
    "ajv": "6.12.6",
    "astring": "1.4.3",
    "autoprefixer": "10.0.1",
    "awesome-typescript-loader": "5.2.1",
    "canvas": "2.6.1",
    "chai": "4.2.0",
    "child_process": "1.0.2",
    "chokidar": "3.4.3",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "fast-async": "6.3.8",
    "file-loader": "6.2.0",
    "fs-extra": "9.0.1",
    "get-random-values": "1.2.2",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "inject-loader": "4.0.1",
    "jasmine": "3.6.3",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mocha": "8.2.1",
    "mochapack": "2.0.6",
    "node-sass": "5.0.0",
    "nodemon": "2.0.6",
    "parse5": "6.0.1",
    "postcss": "8.1.6",
    "postcss-loader": "4.0.4",
    "pretty": "2.0.0",
    "raf-stub": "3.0.0",
    "randomstring": "1.1.5",
    "sass-loader": "10.0.5",
    "sinon": "9.2.1",
    "style-loader": "2.0.0",
    "svg-inline-loader": "0.8.2",
    "terser-webpack-plugin": "5.0.3",
    "typescript": "4.0.5",
    "webpack": "5.4.0",
    "webpack-cli": "4.2.0"
  }
}
