{
  "name": "chrono-trigger-js",
  "version": "1.1.5",
  "description": "An engine that allows arbitrary functionality, defined by a JSON configuration, to be triggered according to a given timeline provider",
  "main": "./lib/library.min.js",
  "types": "./lib/library.d.ts",
  "repository": "https://github.com/rolandzwaga/chrono-trigger.git",
  "author": "Roland Zwaga <rbzwaga@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "mochapack --glob \"*.spec.js\" test --recursive --webpack-config test/webpack.config.js --watch",
    "test-ci": "mochapack --glob \"*.spec.js\" test --recursive --webpack-config test/webpack.config.js",
    "build-example-1": "node build/engine-pack-builder examples/requestanimationframe/config-data.json && webpack --config build/webpack.config.js --env.entryPath examples/requestanimationframe/build/boot.js",
    "full-build": "npm run build && npm run dev-build",
    "build": "webpack --mode production --display-modules",
    "dev-build": "webpack --mode development --progress --colors --display-modules",
    "dev": "webpack --mode development --progress --colors --display-modules --watch",
    "stage-build": "git add lib/library.js lib/library.js.map lib/library.min.js lib/library.d.ts",
    "publish-patch": "npm run full-build && npm run stage-build && git commit -m\"Add new patch build\" && npm version patch",
    "publish-minor": "npm run full-build && npm run stage-build && git commit -m\"Add new minor build\"  && npm version minor",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "d3": "5.9.2",
    "es6-shim": "0.35.5",
    "jquery": "3.4.0",
    "lottie-web": "5.5.1",
    "mediaelement": "4.2.9"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-transform-arrow-functions": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "7.4.3",
    "@types/node": "11.13.6",
    "acorn": "6.1.1",
    "ajv": "6.10.0",
    "astring": "1.4.0",
    "autoprefixer": "9.5.1",
    "babel-loader": "^8.0.5",
    "chai": "4.2.0",
    "child_process": "1.0.2",
    "chokidar": "2.1.5",
    "clean-webpack-plugin": "2.0.1",
    "css-loader": "2.1.1",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "inject-loader": "4.0.1",
    "jasmine": "3.4.0",
    "mocha": "6.1.4",
    "mochapack": "1.1.1",
    "node-sass": "4.11.0",
    "nodemon": "1.18.11",
    "parse5": "5.1.0",
    "postcss": "7.0.14",
    "postcss-loader": "3.0.0",
    "pretty": "^2.0.0",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "sass-loader": "7.1.0",
    "sinon": "^7.3.2",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0"
  }
}
